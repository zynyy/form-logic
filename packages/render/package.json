{
  "name": "@formlogic/render",
  "version": "1.0.0",
  "license": "MIT",
  "browser": "./esm/index.js",
  "types": "esm/index.d.ts",
  "engines": {
    "npm": ">=3.0.0"
  },
  "scripts": {
    "rmBuildDir": "rimraf -rf lib esm dist",
    "build:link": "pnpm run rmBuildDir && pnpm run create:style  && pnpm run build:esm && pnpm run build:style",
    "build": "pnpm run rmBuildDir && pnpm run create:style && pnpm run build:cjs && pnpm run build:esm && pnpm run build:umd && pnpm run build:style",
    "create:style": "ts-node --project tsconfig.cjs.json ./create-style",
    "build:style": "ts-node  --project tsconfig.cjs.json ./build-style",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:umd": "rollup --config",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "run-server": "formlogic-server dev"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@formily/core": "2.2.10",
    "@formily/grid": "2.2.10",
    "@formily/react": "2.2.10",
    "@formily/reactive": "2.2.10",
    "@formily/shared": "2.2.10",
    "@formlogic/component": "^1.0.0",
    "@formlogic/editor": "^1.0.0",
    "@formlogic/server": "^1.0.0",
    "@popperjs/core": "^2.11.6",
    "antd": "^5.1.5",
    "axios": "^1.2.1",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "mustache": "^4.2.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.6.0",
    "scroll-into-view-if-needed": "^3.0.3",
    "store2": "^2.14.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.26"
  },
  "gitHead": "9e3e664d4bc7f9c0527ad8f31e68212b100ae1ca"
}
