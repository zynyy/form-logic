{
  "name": "@formlogic/render-vue",
  "version": "1.0.2-alpha.1",
  "license": "MIT",
  "browser": "esm/index.js",
  "types": "esm/index.d.ts",
  "homepage": "https://github.com/zynyy/form-logic#readme",
  "web-types": "dist/web-types.json",
  "engines": {
    "npm": ">=3.0.0"
  },
  "type": "module",
  "scripts": {
    "rmBuildDir": "rimraf -rf lib esm dist",
    "build:link": "pnpm run rmBuildDir && pnpm run create:style  && pnpm run build:esm && pnpm run build:style",
    "build": "pnpm run rmBuildDir && node --experimental-specifier-resolution=node --experimental-loader ts-node/esm  ./script/build",
    "test": " jest --config jest.config.js",
    "test:usnap": " pnpm test -- -u",
    "create:style": " ts-node --project tsconfig.cjs.json ./create-style",
    "build:style": " ts-node --project tsconfig.cjs.json ./build-style",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:umd": "rollup --config",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "run-server": "formlogic-server dev",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@formily/core": "2.2.10",
    "@formily/grid": "2.2.10",
    "@formily/json-schema": "^2.2.10",
    "@formily/reactive": "2.2.10",
    "@formily/shared": "2.2.10",
    "@formlogic/server": "1.0.2-alpha.1",
    "@vue/shared": "^3.2.47",
    "@vueuse/core": "^9.12.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.2.1",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "consola": "^2.15.3",
    "lodash.frompairs": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "mustache": "^4.2.0",
    "nanospinner": "^1.1.0",
    "qs": "^6.11.0",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^3.0.3",
    "store2": "^2.14.2",
    "vue": "^3.2.47",
    "vue-slicksort": "^2.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/hash-sum": "^1.0.0",
    "@types/less": "^3.0.3",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.47",
    "babel-plugin-import": "^1.13.6",
    "css-loader": "^6.7.3",
    "esbuild": "^0.17.7",
    "execa": "^7.0.0",
    "fast-glob": "^3.2.12",
    "hash-sum": "^2.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "picocolors": "^1.0.0",
    "postcss-load-config": "^4.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "storybook": "^7.0.0-beta.3",
    "style-loader": "^3.3.1",
    "vite": "^4.1.1",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.0.24"
  }
}
